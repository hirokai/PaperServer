       <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x
       <h3 id="myModalLabel">
         Add tags.
     <div class="modal-body">
     <div class="modal-footer">
       <button class="btn" data-dismiss="modal" aria-hidden="true">Close
       <button class="btn btn-primary">Save changes
   <div #hidden-forms .hidden style="display:none;">
     ^{onetimeTokenFW}
   <script type="text/javascript" src=#{asset JQuery}></script>
   <script type="text/javascript" src=#{asset W2UI}></script>
   <script type="text/javascript" src=#{asset BootStrap}></script>
